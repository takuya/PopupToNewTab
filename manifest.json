{
     "manifest_version": 2,

     "name": "PopupToNewTab",
     "description": " This Extension disables POPUP.Instead of PopUp, Open in NewTab"
     "version": "1.0",

     "content_scripts": [
     {
          "matches": [
          "http://*/*",
          "https://*/*"
           ],
          "js": ["contents.js"],
          "run_at": "document_end"
     }
     ]

}
